@{
    ViewBag.Title = "Orders";
}

<h2>@ViewBag.Title.</h2>
<div class="row">
    <div class="col-md-8">
        <div class="btn btn-default">@Html.ActionLink("Create Order", "Create", "Order", routeValues: null, htmlAttributes: new { id = "createOrderLink" })</div>
        <div class="btn btn-default disabled">@Html.ActionLink("Maintain Order", "Create", "Order", routeValues: null, htmlAttributes: new { id = "maintainOrderLink" })</div>
    </div>
    
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}